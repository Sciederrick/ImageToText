<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="ke.derrick.imagetotext.ui.main.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_textResultFragment"
            app:destination="@id/textResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/textResultFragment"
        android:name="ke.derrick.imagetotext.ui.main.TextResultFragment"
        android:label="@string/text_result_fragment"
        tools:layout="@layout/fragment_text_result" >
    </fragment>
</navigation>